pipeline {
    agent any
    tools{
        maven "maven 4.0.0"
    }

    stages {
        stage("Maven clean") {
            when {
                branch "main"
            }
            steps {
                sh "mvc clean"
            }
        }
        stage(" Maven build") {
            when {
                branch "main"
            }
            steps {
                sh "mvn package -DskipTests"
            }
        }
    }
}